{
  "manifest_version": 3,
  "name": "AI IPO Exporter",
  "version": "1.0.0",
  "description": "Export table data from Tofler to CSV",
  "permissions": [],
  "host_permissions": [
    "https://www.tofler.in/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.tofler.in/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
